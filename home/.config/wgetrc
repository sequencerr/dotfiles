# https://www.gnu.org/software/wget/manual/html_node/Wgetrc-Commands.html
# https://www.gnu.org/software/wget/manual/html_node/Sample-Wgetrc.html
# https://github.com/mathiasbynens/dotfiles/blob/main/.wgetrc

hsts-file = ~/.cache/wget-hsts
progress = bar:force:noscroll
quiet = on
tries = 3

header = Accept: */*
header = Accept-Language: en-US,en;q=0.8
header = Keep-Alive: 300
user_agent = Mozilla/5.0 (X11; Linux; en-US; rv:133.0) Gecko/20222219 Firefox/133.0
